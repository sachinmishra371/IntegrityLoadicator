        Title="Zebec-LoadMaster:Loading" Height="128" Width="602" ResizeMode="NoResize" WindowStartupLocation="CenterScreen" Name="frmLoading">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="174*" />
           
        </Grid.RowDefinitions>
        <StackPanel Name="Cofigurationpanel">
            <Label Content="ZebecLoadMaster is Configuring Please wait... " Height="28" Margin="50,16,0,0" Name="label1" Width="296" FontSize="14" />
            <ProgressBar Height="20" HorizontalAlignment="Left"  Margin="150,10,0,0" Name="PrgsLoading" VerticalAlignment="Top" Width="344" Value="70" ValueChanged="PrgsLoading_ValueChanged" />
        </StackPanel>
    </Grid>
</Window>
